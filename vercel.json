{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://mare-alta-python-copy-versel.onrender.com/api/$1"
        },
        {
            "source": "/((?!api/|assets/|favicon.ico|.*\\.).*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "cd frontend && npm install && npm run build",
    "outputDirectory": "frontend/dist"
}